{
  "name": "lollomusicx",
  "productName": "LOLLOMUSICX",
  "version": "0.9.15",
  "description": "LOLLOMUSICX",
  "main": "./out/main/index.js",
  "author": "Lorenzo Orlandini",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --plugin prettier-plugin-svelte --write .",
    "lint": "eslint --cache .",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "build": {
    "appId": "com.lorenzo.lollomusicx",
    "productName": "LOLLOMUSICX",
    "directories": {
      "output": "dist_electron"
    },
    "asar": false,
    "files": [
      "out/**/*"
    ],
    "extraResources": [
      {
        "from": "assets",
        "to": "assets"
      },
      {
        "from": "src/main/appLabel.png",
        "to": "main/appLabel.png"
      }
    ],
    "asarUnpack": [
      "node_modules/yt-dlp-exec/bin/**",
      "node_modules/ffmpeg-static/**",
      "node_modules/*/build/Release/**",
      "node_modules/*/*.node"
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "assets/icon.ico"
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "icon": "assets/icon.icns"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "assets/icon.png"
    }
  },
  "dependencies": {
    "@distube/ytdl-core": "^4.16.11",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "axios": "^1.8.2",
    "child_process": "^1.0.2",
    "crypto": "^1.0.1",
    "discord-rpc": "^4.0.1",
    "electron-store": "^4.0.0",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "googleapis": "^146.0.0",
    "id3-reader": "^0.0.2",
    "music-metadata": "^11.0.0",
    "node-id3": "^0.2.8",
    "node-youtube-music": "^0.10.3",
    "play-dl": "^1.9.7",
    "sharp": "^0.34.1",
    "temp": "^0.9.4",
    "youtube-dl-exec": "^3.0.21",
    "youtube-music-api": "^1.0.6",
    "youtubei.js": "^13.1.0",
    "yt-dlp-exec": "^1.0.2",
    "yt-stream": "^1.7.4",
    "ytdl-core": "^4.11.5",
    "ytdl-mp3": "^5.2.2",
    "ytdlp-nodejs": "^2.2.0",
    "ytdownloader.js": "^1.0.9",
    "ytmusic-api": "^5.3.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^2.0.0",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "electron": "^34.5.8",
    "electron-builder": "^25.1.8",
    "electron-vite": "^3.0.0",
    "eslint": "^9.20.1",
    "eslint-plugin-svelte": "^2.46.1",
    "prettier": "^3.5.1",
    "prettier-plugin-svelte": "^3.3.3",
    "svelte": "^5.20.1",
    "vite": "^6.1.0"
  }
}
